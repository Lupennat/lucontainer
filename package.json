{
    "name": "lucontainer",
    "version": "0.0.1",
    "description": "Service Container for Node js",
    "author": "Claudio Pennati <claudio.pennati@gmail.com>",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Lupennat/lucontainer.git"
    },
    "keywords": [
        "injection",
        "container",
        "service",
        "ioc"
    ],
    "main": "./dist/index.js",
    "types": "./dist/types.d.ts",
    "exports": {
        ".": "./dist/index.js",
        "./errors/*": "./dist/errors/*.js",
        "./types": "./dist/types.d.ts"
    },
    "files": [
        "dist/*"
    ],
    "publishConfig": {
        "access": "public"
    },
    "scripts": {
        "prepublishOnly": "safe-publish-latest && npm run lint && npm run test && npm run build",
        "prepublish": "not-in-publish || npm run prepublishOnly",
        "clean": "del-cli dist",
        "lint": "eslint src",
        "lint:fix": "eslint src --fix",
        "compile": "npm run clean && tsc -p src/tsconfig.json",
        "build": "npm run compile",
        "test": "nyc --reporter=lcov --require ts-node/register mocha test/**/*.test.ts --reporter spec --require 'node_modules/reflect-metadata/Reflect.js' --exit",
        "watch": "tsc -p src/tsconfig.json -w"
    },
    "engines": {
        "node": "^14.19.0 || ^16.10.0 || >=18.0.0"
    },
    "devDependencies": {
        "@tsconfig/node16": "^1.0.3",
        "@types/chai": "^4.3.4",
        "@types/mocha": "^10.0.1",
        "@types/node": "^18.11.11",
        "@typescript-eslint/eslint-plugin": "^5.45.1",
        "chai": "^4.3.7",
        "del-cli": "^5.0.0",
        "eslint": "^8.29.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-config-standard-with-typescript": "^23.0.0",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-n": "^15.6.0",
        "eslint-plugin-promise": "^6.1.1",
        "mocha": "^10.1.0",
        "nyc": "^15.1.0",
        "safe-publish-latest": "^2.0.0",
        "ts-node": "^10.9.1",
        "typescript": "^4.9.3"
    },
    "dependencies": {
        "acorn": "^8.8.1",
        "reflect-metadata": "^0.1.13"
    }
}
